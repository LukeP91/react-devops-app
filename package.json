{
  "name": "react-devops-sample-app",
  "description": "Selleo :: Devops sample React app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next -p 3005",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "classnames": "2.3.1",
    "dayjs": "1.10.7",
    "embla-carousel-react": "5.0.1",
    "js-cookie": "3.0.1",
    "lodash-es": "4.17.21",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.0",
    "@types/lodash-es": "4.17.5",
    "@types/node": "14.17.21",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.0",
    "prettier": "2.4.1",
    "sass": "1.42.1",
    "typescript": "4.4.3"
  }
}
